{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'CustomerCenter',\n  setup() {\n    let about = ref(true);\n    let rule = ref(false);\n    let qna = ref(false);\n    let credit = ref(false);\n    let qnaText = ref('');\n    let qnaList = ref([]);\n    let nickName = ref('');\n    axios.get('/api/customerCenter/nonMemberQna').then(res => {\n      qnaList.value = res.data;\n    }).catch(err => {\n      console.log(err);\n    });\n    function clickAbout() {\n      credit.value = false;\n      about.value = true;\n      rule.value = false;\n      qna.value = false;\n    }\n    function clickRule() {\n      credit.value = false;\n      about.value = false;\n      rule.value = true;\n      qna.value = false;\n    }\n    function clickQna() {\n      credit.value = false;\n      about.value = false;\n      rule.value = false;\n      qna.value = true;\n    }\n    function clickCredit() {\n      credit.value = true;\n      about.value = false;\n      rule.value = false;\n      qna.value = false;\n    }\n    function inputNick(e) {\n      nickName.value = e.target.value;\n    }\n    function inputText(e) {\n      if (e.target.value.length <= 100) {\n        qnaText.value = e.target.value;\n      } else {\n        alert('100자 이내로 입력하시기 바랍니다.');\n        e.target.value = qna.value;\n      }\n    }\n    function qnaSubmit() {\n      if (nickName.value === '') {\n        alert('닉네임을 입력해 주십시오');\n      } else if (qnaText.value === '') {\n        alert('문의 내용을 입력해주십시오.');\n      } else {\n        let qnaData = {\n          nickName: nickName.value,\n          text: qnaText.value\n        };\n        axios.post('/api/customerCenter/qnaSubmit', qnaData).then(result => {\n          qnaText.value = '';\n          alert('등록되었습니다.');\n          qnaList.value = result.data;\n        }).catch(err => {\n          console.log(err);\n        });\n      }\n    }\n    return {\n      clickAbout,\n      clickRule,\n      clickQna,\n      about,\n      qna,\n      rule,\n      qnaText,\n      qnaList,\n      nickName,\n      qnaSubmit,\n      inputText,\n      inputNick,\n      credit,\n      clickCredit\n    };\n  }\n};","map":{"version":3,"mappings":"AAoHA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,KAAK,GAAG;IACN,IAAIC,KAAI,GAAIJ,GAAG,CAAC,IAAI,CAAC;IACrB,IAAIK,IAAG,GAAIL,GAAG,CAAC,KAAK,CAAC;IACrB,IAAIM,GAAE,GAAIN,GAAG,CAAC,KAAK,CAAC;IACpB,IAAIO,MAAK,GAAIP,GAAG,CAAC,KAAK,CAAC;IACvB,IAAIQ,OAAM,GAAIR,GAAG,CAAC,EAAE,CAAC;IACrB,IAAIS,OAAM,GAAIT,GAAG,CAAC,EAAE,CAAC;IACrB,IAAIU,QAAO,GAAIV,GAAG,CAAC,EAAE,CAAC;IAGtBC,KAAK,CAACU,GAAG,CAAC,kCAAkC,EAC3CC,IAAI,CAACC,GAAG,IAAE;MACTJ,OAAO,CAACK,KAAI,GAAID,GAAG,CAACE,IAAI;IAC1B,CAAC,EACAC,KAAK,CAACC,GAAG,IAAE;MACVC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IAClB,CAAC;IACD,SAASG,UAAU,GAAG;MACpBb,MAAM,CAACO,KAAI,GAAI,KAAK;MACpBV,KAAK,CAACU,KAAI,GAAI,IAAI;MAClBT,IAAI,CAACS,KAAI,GAAI,KAAK;MAClBR,GAAG,CAACQ,KAAI,GAAI,KAAK;IACnB;IAEA,SAASO,SAAS,GAAG;MACnBd,MAAM,CAACO,KAAI,GAAI,KAAK;MACpBV,KAAK,CAACU,KAAI,GAAI,KAAK;MACnBT,IAAI,CAACS,KAAI,GAAI,IAAI;MACjBR,GAAG,CAACQ,KAAI,GAAI,KAAK;IACnB;IAEA,SAASQ,QAAQ,GAAG;MAClBf,MAAM,CAACO,KAAI,GAAI,KAAK;MACpBV,KAAK,CAACU,KAAI,GAAI,KAAK;MACnBT,IAAI,CAACS,KAAI,GAAI,KAAK;MAClBR,GAAG,CAACQ,KAAI,GAAI,IAAI;IAClB;IAEA,SAASS,WAAW,GAAG;MACrBhB,MAAM,CAACO,KAAI,GAAI,IAAI;MACnBV,KAAK,CAACU,KAAI,GAAI,KAAK;MACnBT,IAAI,CAACS,KAAI,GAAI,KAAK;MAClBR,GAAG,CAACQ,KAAI,GAAI,KAAK;IACnB;IAEA,SAASU,SAAS,CAACC,CAAC,EAAE;MACpBf,QAAQ,CAACI,KAAI,GAAIW,CAAC,CAACC,MAAM,CAACZ,KAAK;IACjC;IAEA,SAASa,SAAS,CAACF,CAAC,EAAE;MACpB,IAAGA,CAAC,CAACC,MAAM,CAACZ,KAAK,CAACc,MAAK,IAAK,GAAG,EAAE;QAC/BpB,OAAO,CAACM,KAAI,GAAIW,CAAC,CAACC,MAAM,CAACZ,KAAK;MAChC,OAAO;QACLe,KAAK,CAAC,sBAAsB,CAAC;QAC7BJ,CAAC,CAACC,MAAM,CAACZ,KAAI,GAAIR,GAAG,CAACQ,KAAK;MAC5B;IACF;IAEA,SAASgB,SAAS,GAAG;MACnB,IAAGpB,QAAQ,CAACI,KAAI,KAAM,EAAE,EAAE;QACxBe,KAAK,CAAC,eAAe,CAAC;MACxB,OAAO,IAAGrB,OAAO,CAACM,KAAI,KAAM,EAAE,EAAE;QAC9Be,KAAK,CAAC,iBAAiB,CAAC;MAC1B,OAAO;QACL,IAAIE,OA