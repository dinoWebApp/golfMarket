oduct.value.productName,\n          productId : product.value.id,\n          text : qnaText.value\n        }\n        axios.post('/api/product/qnaSubmit', qnaData)\n        .then((result)=>{\n          if(result.data === 'need login') {\n            alert('로그인이 필요합니다.');\n            qnaText.value = '';\n          } else {\n            qnaText.value = '';\n            alert('등록되었습니다.');\n            qnaList.value = result.data;\n          }\n        })\n        .catch(err=>{\n          console.log(err);\n        })\n      }\n    }\n    return {fittingText, inputText};\n  }\n\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}