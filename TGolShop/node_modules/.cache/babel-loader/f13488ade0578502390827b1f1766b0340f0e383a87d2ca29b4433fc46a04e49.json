{"ast":null,"code":"import axios from 'axios';\nimport { ref } from 'vue';\nexport default {\n  name: 'FittingPage',\n  setup() {\n    let fittingText = ref('');\n    let fittingList = ref([]);\n    axios.get('/api/fitting').then(res => {\n      if (res.data === 'need login') alert('로그인이 필요합니다.');\n    });\n    function inputText(e) {\n      if (e.target.value.length <= 100) {\n        fittingText.value = e.target.value;\n      } else {\n        alert('100자 이내로 입력하시기 바랍니다.');\n        e.target.value = fittingText.value;\n      }\n    }\n    function fittingSubmit() {\n      if (fittingText.value === '') {\n        alert('문의 내용을 입력해주십시오.');\n      } else {\n        let fittingData = {\n          text: fittingText.value\n        };\n        axios.post('/api/fitting/submit', fittingData).then(result => {\n          fittingText.value = '';\n          alert('등록되었습니다.');\n          fittingList.value = result.data;\n        }).catch(err => {\n          console.log(err);\n        });\n      }\n    }\n    return {\n      fittingText,\n      inputText,\n      fittingSubmit,\n      fittingList\n    };\n  }\n};","map":{"version":3,"mappings":"AA0DA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAE,QAAS,KAAK;AACzB,eAAe;EACbC,IAAI,EAAC,aAAa;EAClBC,KAAK,GAAG;IACN,IAAIC,WAAU,GAAIH,GAAG,CAAC,EAAE,CAAC;IACzB,IAAII,WAAU,GAAIJ,GAAG,CAAC,EAAE,CAAC;IAGzBD,KAAK,CAACM,GAAG,CAAC,cAAc,EACvBC,IAAI,CAACC,GAAG,IAAE;MACT,IAAGA,GAAG,CAACC,IAAG,KAAM,YAAY,EAAEC,KAAK,CAAC,aAAa,CAAC;IACpD,CAAC;IAGD,SAASC,SAAS,CAACC,CAAC,EAAE;MACpB,IAAGA,CAAC,CAACC,MAAM,CAACC,KAAK,CAACC,MAAK,IAAK,GAAG,EAAE;QAC/BX,WAAW,CAACU,KAAI,GAAIF,CAAC,CAACC,MAAM,CAACC,KAAK;MACpC,OAAO;QACLJ,KAAK,CAAC,sBAAsB,CAAC;QAC7BE,CAAC,CAACC,MAAM,CAACC,KAAI,GAAIV,WAAW,CAACU,KAAK;MACpC;IACF;IAEA,SAASE,aAAa,GAAG;MACvB,IAAGZ,WAAW,CAACU,KAAI,KAAM,EAAE,EAAE;QAC3BJ,KAAK,CAAC,iBAAiB,CAAC;MAC1B,OAAO;QACL,IAAIO,WAAU,GAAI;UAChBC,IAAG,EAAId,WAAW,CAACU;QACrB;QACAd,KAAK,CAACmB,IAAI,CAAC,qBAAqB,EAAEF,WAAW,EAC5CV,IAAI,CAAEa,MAAM,IAAG;UACZhB,WAAW,CAACU,KAAI,GAAI,EAAE;UA