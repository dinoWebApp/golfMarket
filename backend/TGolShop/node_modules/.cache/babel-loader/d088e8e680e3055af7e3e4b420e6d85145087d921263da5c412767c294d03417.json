{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from '@vue/reactivity';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name: 'HomePage',\n  setup() {\n    const router = useRouter();\n    let loginInfo = ref(false);\n    let myInfo = ref(false);\n    let nickName = ref('');\n    let profileModal = ref(false);\n    let searchText = ref('');\n    axios.get('/api/customer/login-check').then(res => {\n      console.log(res.data);\n      if (res.data === 'not login') {\n        loginInfo.value = true;\n        myInfo.value = false;\n      } else {\n        nickName.value = res.data;\n        loginInfo.value = false;\n        myInfo.value = true;\n      }\n    });\n    function login() {\n      router.push({\n        path: '/customer/login'\n      });\n    }\n    function signUp() {\n      router.push({\n        path: '/customer/sign-up'\n      });\n    }\n    function logout() {\n      axios.get('/api/customer/logout').then(res => {\n        console.log(res.data);\n        if (res.data === 'logout success') {\n          alert('로그아웃 되었습니다.');\n          loginInfo.value = true;\n          myInfo.value = false;\n          router.push({\n            path: '/'\n          });\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n    function profileLogout() {\n      axios.get('/api/customer/logout').then(res => {\n        console.log(res.data);\n        if (res.data === 'logout success') {\n          alert('로그아웃 되었습니다.');\n          loginInfo.value = true;\n          myInfo.value = false;\n          router.push({\n            path: '/'\n          });\n          profileModal.value = false;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n    function loginUpdate(loginData) {\n      nickName.value = loginData[0];\n      loginInfo.value = loginData[1];\n      myInfo.value = loginData[2];\n    }\n    function profileClick() {\n      axios.get('/api/customer/login-check').then(res => {\n        if (res.data === 'not login') {\n          alert('로그인이 필요합니다.');\n          router.push({\n            path: '/customer/login'\n          });\n        } else profileModal.value = true;\n      });\n    }\n    function profileDelete() {\n      profileModal.value = false;\n    }\n    function mypage() {\n      router.push({\n        name: 'MyPage',\n        query: {\n          nickName: nickName.value,\n          cart: 0\n        }\n      });\n    }\n    function inputSearch(e) {\n      searchText.value = e.target.value;\n    }\n    function clickSearch() {\n      router.push({\n        name: 'SearchPage',\n        query: {\n          searchText: searchText.value\n        }\n      });\n    }\n    function clickDivide(e) {\n      let korBrand = e.target.innerText;\n      let engBrand = '';\n      switch (e.target.innerText) {\n        case '드라이버':\n          engBrand = 'driver';\n          break;\n        case '우드/유틸':\n          engBrand = 'wu';\n          break;\n        case '아이언':\n          engBrand = 'iron';\n          break;\n        case '웨지':\n          engBrand = 'wedge';\n          break;\n        case '퍼터':\n          engBrand = 'putter';\n          break;\n        case '골프백/볼/기타':\n          engBrand = 'etc';\n          break;\n        case '헤드/샤프트':\n          engBrand = 'headShaft';\n          break;\n        case '풀세트':\n          engBrand = 'fullSet';\n          break;\n      }\n      router.push({\n        name: 'ProductsPage'\n      });\n    }\n    return {\n      login,\n      signUp,\n      loginInfo,\n      myInfo,\n      nickName,\n      logout,\n      loginUpdate,\n      profileClick,\n      profileModal,\n      profileDelete,\n      profileLogout,\n      mypage,\n      searchText,\n      inputSearch,\n      clickSearch,\n      clickDivide\n    };\n  }\n};","map":{"version":3,"mappings":";AA2GA,SAASA,GAAE,QAAS,iBAAiB;AACrC,SAASC,SAAQ,QAAS,YAAW;AACrC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,U