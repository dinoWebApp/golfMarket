{"ast":null,"code":"import { ref } from '@vue/reactivity';\nimport { useRoute } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name: 'MyPage',\n  setup() {\n    const route = useRoute();\n    let nickName = ref(route.query.nickName);\n    let mypageData = ref({});\n    let updateKey = ref(0);\n    let purchaseList = ref(true);\n    let cart = ref(false);\n    let deliInfo = ref(false);\n    let qna = ref(false);\n    let point = ref(false);\n    let info = ref(false);\n    let reviews = ref(false);\n    axios.get('/api/customer/mypage?nickName=' + route.query.nickName).then(res => {\n      mypageData.value = res.data;\n      if (route.query.cart === '1') {\n        updateKey.value++;\n      }\n    }).catch(err => {\n      console.log(err);\n    });\n    function filter(val) {\n      return String(val).replace(/\\B(?=(\\d{3})+(?!\\d))/g, ',');\n    }\n    function clickPurchaseData() {\n      purchaseList.value = true;\n      deliInfo.value = false;\n      cart.value = false;\n      reviews.value = false;\n      qna.value = false;\n      point.value = false;\n      info.value = false;\n    }\n    function clickDeliInfo() {\n      purchaseList.value = false;\n      deliInfo.value = true;\n      cart.value = false;\n      reviews.value = false;\n      qna.value = false;\n      point.value = false;\n      info.value = false;\n    }\n    function clickCart() {\n      purchaseList.value = false;\n      deliInfo.value = false;\n      cart.value = true;\n      reviews.value = false;\n      qna.value = false;\n      point.value = false;\n      info.value = false;\n    }\n    function clickReviews() {\n      purchaseList.value = false;\n      deliInfo.value = false;\n      cart.value = false;\n      reviews.value = true;\n      qna.value = false;\n      point.value = false;\n      info.value = false;\n    }\n    function clickQna() {\n      purchaseList.value = false;\n      deliInfo.value = false;\n      cart.value = false;\n      reviews.value = true;\n      qna.value = false;\n      point.value = false;\n      info.value = false;\n    }\n    return {\n      nickName,\n      updateKey,\n      purchaseList,\n      cart,\n      mypageData,\n      filter,\n      deliInfo,\n      reviews,\n      qna,\n      point,\n      info,\n      clickPurchaseData,\n      clickDeliInfo,\n      clickCart,\n      clickReviews\n    };\n  }\n};","map":{"version":3,"mappings":"AAmGA,SAASA,GAAE,QAAS,iBAAiB;AACrC,SAASC,QAAQ,QAAO,YAAW;AACnC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,EAAE;IACxB,IAAIK,QAAO,GAAIN,GAAG,CAACK,KAAK,CAACE,KAAK,CAACD,QAAQ,CAAC;IACxC,IAAIE,UAAS,GAAIR,GAAG,CAAC,CAAC,CAAC,CAAC;IACxB,IAAIS,SAAQ,GAAIT,GAAG,CAAC,CAAC,CAAC;IACtB,IAAIU,YAAW,GAAIV,GAAG,CAAC,IAAI,CAAC;IAC5B,IAAIW,IAAG,GAAIX,GAAG,CAAC,KAAK,CAAC;IACrB,IAAIY,QAAO,GAAIZ,GAAG,CAAC,KAAK,CAAC;IACzB,IAAIa,GAAE,GAAIb,GAAG,CAAC,KAAK,CAAC;IACpB,IAAIc,KAAI,GAAId,GAAG,CAAC,KAAK,CAAC;IACtB,IAAIe,IAAG,GAAIf,GAAG,CAAC,KAAK,CAAC;IACrB,IAAIgB,OAAM,GAAIhB,GAAG,CAAC,KAAK,CAAC;IAGxBE,KAAK,CAACe,GAAG,CAAC,gCAA+B,GAAIZ,KAAK,CAACE,KAAK,CAACD,QAAQ,EAChEY,IAAI,CAACC,GAAG,IAAE;MACTX,UAAU,CAACY,KAAI,GAAID,GAAG,CAACE,IAAI;MAC3B,IAAGhB,KAAK,CAACE,KAAK,CAACI,IAAG,KAAM,GAAG,EAAE;QAC3BF,SAAS,CAACW,KAAK,EAAE;MACnB;IACF,CAAC,EACAE,KAAK,CAACC,GAAG,IAAE;MACVC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IAClB,CAAC;IAED,SAASG,MAAM,CAACC,GAAG,EAAE;MACnB,OAAOC,MAAM,CAACD,GAAG,CAAC,CAACE,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IAC1D;IACA,SAASC,iBAAiB,GAAG;MAC3BpB,YAAY,CAACU,KAAI,GAAI,IAAI;MACzBR,QAAQ,CAACQ,KAAI,GAAI,KAAK;MACtBT,IAAI,CAACS,KAAI,GAAI,KAAK;MAClBJ,OAAO,CAACI,KAAI,GAAI,KAAK;MACrBP,GAAG,CAACO,KAAI,GAAI,KAAK;MACjBN,KAAK,CAACM,KAAI,GAAI,KAAK;MACnBL,IAAI,CAACK,KAAI,GAAI,KAAK;IACpB;IAEA,SAASW,aAAa,GAAG;MACvBrB,YAAY,CAACU,KAAI,GAAI,KAAK;MAC1BR,QAAQ,CAACQ,KAAI,GAAI,IAAI;MACrBT,IAAI,CAACS,KAAI,GAAI,KAAK;MAClBJ,OAAO,CAACI,KAAI,GAAI,KAAK;MACrBP,GAAG,CAACO,KAAI,GAAI,KAAK;MACjBN,KAAK,CAACM,KAAI,GAAI,KAAK;MACnBL,IAAI,CAACK,KAAI,GAAI,KAAK;IACpB;IAEA,SAASY,SAAS,GAAG;M