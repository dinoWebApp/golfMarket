sole.log(infoImage.value);\n    }\n\n    return {adminData, pw, submit, productEnroll, divide, productDivide, brandChoice, productName, inputProductName, inputProductPrice,\n    };\n  }\n}\n</script>\n\n<style>\n#admin {\n  position: fixed;\n  left: 40%;\n  right: 40%;\n  top: 40%;\n}\n\n#sidebar{\n  height: 100vh !important;\n}\n\n\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}