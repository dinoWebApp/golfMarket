pe=\"button\" role=\"tab\" aria-selected=\"false\">야마하</button>\n        <button class=\"nav-link check\" data-bs-toggle=\"tab\" type=\"button\" role=\"tab\" aria-selected=\"false\">미즈노</button>\n        <button class=\"nav-link check\" data-bs-toggle=\"tab\" type=\"button\" role=\"tab\" aria-selected=\"false\">포틴</button>\n        <button class=\"nav-link check\" data-bs-toggle=\"tab\" type=\"button\" role=\"tab\" aria-selected=\"false\">마제스티</button>\n      </div>\n    </nav>\n  </div>\n\n  <div class=\"container\">\n    <h3 class=\"mt-3 mb-3\" style=\"text-align:left;\">드라이버</h3>\n  </div>\n  \n\n  <div class=\"container\">\n\n      <div class=\"row row-cols-2 row-cols-sm-2 row-cols-md-3 row-cols-lg-3 row-cols-xl-4 g-4\">\n        <div class=\"col\" v-for=\"item in products\" :key=\"item\">\n          <div class=\"card shadow-sm\">\n            <div id=\"img-border\">\n              <img id=\"image\" :src='`http://localhost:3000/${item.thumbnail}`' alt=\"logo\" class=\"img-fluid img-thumbnail\">\n            </div>\n            \n            <div class=\"card-body\">\n              <div id=\"text-border1\" class=\"d-lg-none\">\n                <p id=\"product-text\" class=\"card-text\" style=\"font-weight:500;\"> {{item.productName}} </p>\n              </div>\n              <div id=\"text-border2\" class=\"d-none d-lg-block\">\n                <p id=\"product-text\" class=\"card-text\" style=\"font-weight:500;\"> {{item.productName}} </p>\n              </div>\n              \n              <p style=\"font-weight:900; font-size:20px;\"> {{filter(item.productPrice)}} 원 </p>\n              <div class=\"d-flex justify-content-between align-items-center\">\n                <div class=\"btn-group\">\n                  <button type=\"button\" class=\"btn btn-sm btn-success\" style=\"font-weight:bold;\">장바구니</button>\n                  <button type=\"button\" class=\"btn btn-sm btn-danger\" style=\"font-weight:bold;\">구매하기</button>\n                </div>\n                <small style=\"font-weight:bold; color:red !important;\" v-if=\"item.deliverOut\" class=\"text-muted\">[해외직구]</small>\n              </div>\n            </div>\n          </div>\n        </div>\n        \n      </div>\n    </div>\n  </div>\n  \n  \n</template>\n\n<script>\nimport axios from 'axios'\nimport { ref } from '@vue/reactivity';\nexport default {\n  name:'DriverPage',\n  \n  setup() {\n    let products = ref([]);\n    axios.get('/api/product/driver').then(res=>{\n      console.log(res.data);\n      products.value = res.data;\n    }). catch(err=>{\n      console.log(err);\n    })\n\n    function filter(val) {\n      return String(val).replace(/\\B(?=(\\d{3})+(?!\\d))/g, ',');\n    }\n\n    function brandClicked(e) {\n      let brandName = e.target.innerHTML;\n      let brandEng;\n      console.log(brandName);\n      switch(brandName) {\n        case '핑':\n          brandEng = 'ping';\n          break;\n        case '테일러메이드':\n          brandEng = 'taylormade';\n          break;\n        case 'PXG':\n          brandEng = 'pxg';\n          break;\n        case '타이틀리스트':\n          brandEng = 'titleist';\n          break;\n        case '혼마':\n          brandEng = 'honma';\n          break;\n        case '젝시오':\n          brandEng = 'xxio';\n          break;\n        case '핑':\n          brandEng = 'ping';\n          break;\n        case '핑':\n          brandEng = 'ping';\n          break;  \n      }\n      axios.get('/api/product/driver/')\n    }\n    return {products, filter, brandClicked};\n\n  }\n}\n</script>\n\n<style>\n\n.nav-link.active{\n  font-weight: bold;\n}\n\n.nav-link.check {\n  color:darkgray ;\n  \n}\n\n\n.nav-item .nav-link{\n  font-weight: bold;\n}\n\n#side{\n  color: darkgrey;\n}\n\n#image {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  \n}\n\n#img-border {\n  position: relative;\n  width: 100%;\n}\n\n#img-border::after {\n  display: block;\n  content: '';\n  padding-bottom: 100%;\n}\n\n#text-border1 {\n  position: relative;\n  width: 100%;\n}\n\n#text-border1::after {\n  display: block;\n  content: '';\n  padding-bottom: 40%;\n}\n\n#text-border2 {\n  position: relative;\n  width: 100%;\n}\n\n#text-border2::after {\n  display: block;\n  content: '';\n  padding-bottom: 20%;\n}\n\n#product-text {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  \n}\n  \n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}