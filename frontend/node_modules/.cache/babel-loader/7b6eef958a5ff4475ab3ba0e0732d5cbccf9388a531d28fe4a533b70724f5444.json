{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from '@vue/reactivity';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name: 'LoginPage',\n  emits: ['loginData'],\n  setup(props, {\n    emit\n  }) {\n    const router = useRouter();\n    let id = ref();\n    let pw = ref();\n    let loginData = ref({\n      id: id,\n      pw: pw\n    });\n    axios.get('/api/customer/login-check').then(res => {\n      console.log(res.data);\n      if (res.data !== 'not login') {\n        router.push({\n          path: '/'\n        });\n      }\n    });\n    function signUp() {\n      router.push({\n        path: '/customer/sign-up'\n      });\n    }\n    function login() {\n      axios.post('/api/customer/login', loginData.value).then(res => {\n        console.log(res.data);\n        if (res.data === 'login success') {\n          emit('loginData', [false, true]);\n          router.push({\n            path: '/'\n          });\n        } else alert('아이디 또는 비밀번호가 일치하지 않습니다.');\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n    return {\n      signUp,\n      login,\n      id,\n      pw\n    };\n  }\n};","map":{"version":3,"mappings":";AAkCA,SAASA,GAAE,QAAS,iBAAiB;AACrC,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAC,WAAW;EAChBC,KAAK,EAAC,CAAC,WAAW,CAAC;EACnBC,KAAK,CAACC,KAAK,EAAE;IAACC;EAAI,CAAC,EAAE;IACnB,MAAMC,MAAK,GAAIP,SAAS,EAAE;IAC1B,IAAIQ,EAAC,GAAIT,GAAG,EAAE;IACd,IAAIU,EAAC,GAAIV,GAAG,EAAE;IACd,IAAIW,SAAQ,GAAIX,GAAG,CAAC;MAClBS,EAAC,EAAIA,EAAE;MACPC,EAAC,EAAIA;IACP,CAAC,CAAC;IAEFR,KAAK,CAACU,GAAG,CAAC,2BAA2B,EACpCC,IAAI,CAACC,GAAG,IAAE;MACTC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;MACrB,IAAGH,GAAG,CAACG,IAAG,KAAM,WAAW,EAAE;QAC3BT,MAAM,CAACU,IAAI,CAAC;UAACC,IAAI,EAAC;QAAG,CAAC,CAAC;MACzB;IACF,CAAC;IAED,SAASC,MAAM,GAAG;MAChBZ,MAAM,CAACU,IAAI,CAAC;QAACC,IAAI,EAAC;MAAmB,CAAC,CAAC;IACzC;IAEA,SAASE,KAAK,GAAG;MACfnB,KAAK,CAACoB,IAAI,CAAC,qBAAqB,EAAEX,SAAS,CAACY,KAAK,EAChDV,IAAI,CAACC,GAAG,IAAE;QACTC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;QACrB,IAAIH,GAAG,CAACG,IAAG,KAAM,eAAe,EAAE;UAChCV,IAAI,CAAC,WAAW,EAAE,CAAC,KAAK,EAAE,IAAI,CAAC,CAAC;UAChCC,MAAM,CAACU,IAAI,CAAC;YAACC,IAAI,EAAC;UAAG,CAAC,CAAC;QACzB,OAAOK,KAAK,CAAC,yBAAyB;MACxC,CAAC,CAAC,CAACC,KAAK,CAACC,GAAG,IAAE;QACZX,OAAO,CAACC,GAAG,CAACU,GAAG,CAAC;MAClB,CAAC;IACH;IAIA,OAAM;MAACN,MAAM;MAAEC,KAAK;MAAEZ,EAAE;MAAEC;IAAE,CAAC;EAC/B;AACF","names":["ref","useRouter","axios","name","emits","setup","props","emit","router","id","pw","loginData","get","then","res","console","log","data","push","path","signUp","login","post","value","alert","catch","err"],"sourceRoot":"","sources":["/Users/dino/Library/Mobile Documents/com~apple~CloudDocs/공유/project/tgolshop/frontend/src/components/LoginPage.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-3 col-9 col-sm-7 col-md-6 col-lg-5 col-xxl-4 border\">\n    \n      <div class=\"mt-5\">\n        <img class=\"mb-4 col-6 col-md-6\" src=\"../assets/tgolshop.png\" alt=\"dd\">\n      </div>\n      \n      <h1 class=\"h3 mb-3 fw-normal\">로그인</h1>\n\n      <div class=\"form-floating mb-2\">\n        <input type=\"text\" class=\"form-control\" id=\"floatingInput\" placeholder=\"아이디\" v-model=\"id\">\n        <label for=\"floatingInput\">아이디</label>\n      </div>\n      <div class=\"form-floating\">\n        <input type=\"password\" class=\"form-control\" id=\"floatingPassword\" placeholder=\"패스워드\" autocomplete=\"off\" v-model=\"pw\">\n        <label for=\"floatingPassword\">패스워드</label>\n      </div>\n\n      \n      <button @click=\"login\" class=\"w-100 btn btn-lg btn-success mt-4 mb-3\" style=\"font-weight:bold;\">로그인</button>\n      <button class=\"w-100 btn btn-lg mb-3\" @click=\"signUp\" id=\"naver\" style=\"color:white; font-weight:bold; background-color:#12D308;\">간편 회원가입</button>\n\n      \n      <!-- <div class=\"checkbox mb-3\">\n        <label>\n          <input type=\"checkbox\" value=\"remember-me\"> Remember me\n        </label>\n      </div> -->\n\n    \n  </div>\n</template>\n\n<script>\nimport { ref } from '@vue/reactivity';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name:'LoginPage',\n  emits:['loginData'],\n  setup(props, {emit}) {\n    const router = useRouter();\n    let id = ref();\n    let pw = ref();\n    let loginData = ref({\n      id : id,\n      pw : pw\n    });\n\n    axios.get('/api/customer/login-check')\n    .then(res=>{\n      console.log(res.data);\n      if(res.data !== 'not login') {\n        router.push({path:'/'});\n      }\n    })\n\n    function signUp() {\n      router.push({path:'/customer/sign-up'});\n    }\n\n    function login() {\n      axios.post('/api/customer/login', loginData.value)\n      .then(res=>{\n        console.log(res.data);\n        if (res.data === 'login success') {\n          emit('loginData', [false, true]);\n          router.push({path:'/'});\n        } else alert('아이디 또는 비밀번호가 일치하지 않습니다.')\n      }).catch(err=>{\n        console.log(err);\n      })\n    }\n\n\n\n    return{signUp, login, id, pw};\n  }\n}\n</script>\n\n<style>\n  #naver:hover{\n    background-color: #07B70D !important;\n  }\n  #kakao:hover{\n    background-color: #d8b208!important;\n  }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}