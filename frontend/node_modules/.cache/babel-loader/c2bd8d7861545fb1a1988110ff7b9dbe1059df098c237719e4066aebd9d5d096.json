{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from '@vue/reactivity';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name: 'LoginPage',\n  setup() {\n    const router = useRouter();\n    let id = ref();\n    let pw = ref();\n    let loginData = ref({\n      id: id,\n      pw: pw\n    });\n    function signUp() {\n      router.push({\n        path: '/customer/sign-up'\n      });\n    }\n    function login() {\n      axios.post('/api/customer/login', loginData.value).then(res => {\n        console.log(res.data);\n        if (res.data === 'login success') router.push({\n          path: '/api/'\n        });\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n    return {\n      signUp,\n      id,\n      pw\n    };\n  }\n};","map":{"version":3,"mappings":";AAkCA,SAASA,GAAE,QAAS,iBAAiB;AACrC,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAC,WAAW;EAChBC,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIJ,SAAS,EAAE;IAC1B,IAAIK,EAAC,GAAIN,GAAG,EAAE;IACd,IAAIO,EAAC,GAAIP,GAAG,EAAE;IACd,IAAIQ,SAAQ,GAAIR,GAAG,CAAC;MAClBM,EAAC,EAAIA,EAAE;MACPC,EAAC,EAAIA;IACP,CAAC,CAAC;IACF,SAASE,MAAM,GAAG;MAChBJ,MAAM,CAACK,IAAI,CAAC;QAACC,IAAI,EAAC;MAAmB,CAAC,CAAC;IACzC;IAEA,SAASC,KAAK,GAAG;MACfV,KAAK,CAACW,IAAI,CAAC,qBAAqB,EAAEL,SAAS,CAACM,KAAK,EAChDC,IAAI,CAACC,GAAG,IAAE;QACTC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;QACrB,IAAIH,GAAG,CAACG,IAAG,KAAM,eAAe,EAAEd,MAAM,CAACK,IAAI,CAAC;UAACC,IAAI,EAAC;QAAO,CAAC,CAAC;MAC/D,CAAC,CAAC,CAACS,KAAK,CAACC,GAAG,IAAE;QACZJ,OAAO,CAACC,GAAG,CAACG,GAAG,CAAC;MAClB,CAAC;IACH;IAIA,OAAM;MAACZ,MAAM;MAAEH,EAAE;MAAEC;IAAE,CAAC;EACxB;AACF","names":["ref","useRouter","axios","name","setup","router","id","pw","loginData","signUp","push","path","login","post","value","then","res","console","log","data","catch","err"],"sourceRoot":"","sources":["/Users/dino/Library/Mobile Documents/com~apple~CloudDocs/공유/project/tgolshop/frontend/src/components/LoginPage.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-3 col-9 col-sm-7 col-md-6 col-lg-5 col-xxl-4 border\">\n    <form>\n      <div class=\"mt-5\">\n        <img class=\"mb-4 col-6 col-md-6\" src=\"../assets/tgolshop.png\" alt=\"dd\">\n      </div>\n      \n      <h1 class=\"h3 mb-3 fw-normal\">로그인</h1>\n\n      <div class=\"form-floating mb-2\">\n        <input type=\"email\" class=\"form-control\" id=\"floatingInput\" placeholder=\"name@example.com\" v-model=\"id\">\n        <label for=\"floatingInput\">아이디</label>\n      </div>\n      <div class=\"form-floating\">\n        <input type=\"password\" class=\"form-control\" id=\"floatingPassword\" placeholder=\"Password\" v-model=\"pw\">\n        <label for=\"floatingPassword\">패스워드</label>\n      </div>\n\n      \n      <button class=\"w-100 btn btn-lg btn-success mt-4 mb-3\" style=\"font-weight:bold;\">로그인</button>\n      <button class=\"w-100 btn btn-lg mb-3\" @click=\"signUp\" id=\"naver\" style=\"color:white; font-weight:bold; background-color:#12D308;\">간편 회원가입</button>\n\n      \n      <!-- <div class=\"checkbox mb-3\">\n        <label>\n          <input type=\"checkbox\" value=\"remember-me\"> Remember me\n        </label>\n      </div> -->\n\n    </form>\n  </div>\n</template>\n\n<script>\nimport { ref } from '@vue/reactivity';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name:'LoginPage',\n  setup() {\n    const router = useRouter();\n    let id = ref();\n    let pw = ref();\n    let loginData = ref({\n      id : id,\n      pw : pw\n    });\n    function signUp() {\n      router.push({path:'/customer/sign-up'});\n    }\n\n    function login() {\n      axios.post('/api/customer/login', loginData.value)\n      .then(res=>{\n        console.log(res.data);\n        if (res.data === 'login success') router.push({path:'/api/'});\n      }).catch(err=>{\n        console.log(err);\n      })\n    }\n\n\n\n    return{signUp, id, pw};\n  }\n}\n</script>\n\n<style>\n  #naver:hover{\n    background-color: #07B70D !important;\n  }\n  #kakao:hover{\n    background-color: #d8b208!important;\n  }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}