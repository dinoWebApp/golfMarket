d = e.target.value;\n      router.push({\n        name: 'PurchasePage',\n        params: {\n          id: productId\n        }\n      });\n    }\n    return {\n      products,\n      filter,\n      clickedBrand,\n      clickedTotal,\n      purchase\n    };\n  }\n};","map":{"version":3,"mappings":";AA+DA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,GAAE,QAAS,iBAAiB;AACrC,SAASC,SAAQ,QAAS,YAAY;AACtC,eAAe;EACbC,IAAI,EAAC,YAAY;EAEjBC,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIH,SAAS,EAAE;IAC1B,IAAII,QAAO,GAAIL,GAAG,CAAC,EAAE,CAAC;IACtBD,KAAK,CAACO,GAAG,CAAC,qBAAqB,CAAC,CAACC,IAAI,CAACC,GAAG,IAAE;MACzCC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;MACrBN,QAAQ,CAACO,KAAI,GAAIJ,GAAG,CAACG,IAAI;IAC3B,CAAC,CAAC,CAAEE,KAAK,CAACC,GAAG,IAAE;MACbL,OAAO,CAACC,GAAG,CAACI,GAAG,CAAC;IAClB,CAAC;IAED,SAASC,MAAM,CAACC,GAAG,EAAE;MACnB,OAAOC,MAAM,CAACD,GAAG,CAAC,CAACE,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IAC1D;IAEA,SAASC,YAAY,CAACC,CAAC,EAAE;MACvB,IAAIC,SAAQ,GAAID,CAAC,CAACE,MAAM,CAACC,SAAS;MAClC,IAAIC,QAAQ;MACZf,OAAO,CAACC,GAAG,CAACW,SAAS,CAAC;MACtB,QAAOA,SAAS;QACd,KAAK,GAAG;UACNG,QAAO,GAAI,MAAM;UACjB;QACF,KAAK,QAAQ;UACXA,QAAO,GAAI,YAAY;UACvB;QACF,KAAK,KAAK;UACRA,QAAO,GAAI,KAAK;UAChB;QACF,KAAK,QAAQ;UACXA,QAAO,GAAI,UAAU;UACrB;QACF,KAAK,IAAI;UACPA,QAAO,GAAI,OAAO;UAClB;QACF,KAAK,KAAK;UACRA,QAAO,GAAI,MAAM;UACjB;QACF,KAAK,KAAK;UACRA,QAAO,GAAI,QAAQ;UACnB;QACF,KAAK,KAAK;UACRA,QAAO,GAAI,QAAQ;UACnB;QACF,KAAK,IAAI;UACPA,QAAO,GAAI,UAAU;UACrB;QACF,KAAK,MAAM;UACTA,QAAO,GAAI,SAAS;UACpB;MAAK;MAETf,OAAO,CAACC,GAAG,CAACc,QAAQ,CAAC;MACrBzB,KAAK,CAACO,GAAG,CAAC,sCAAqC,GAAIkB,QAAQ,EAC1DjB,IAAI,CAACC,GAAG,IAAE;QACTC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;QACrBN,QAAQ,CAACO,KAAI,GAAIJ,GAAG,CAACG,IAAI;MAC3B,CAAC,EACAE,KAAK,CAACC,GAAG,IAAE;QACVL,OAAO,CAACC,GAAG,CAACI,GAAG,CAAC;MAClB,CAAC;IACH;IAEA,SAASW,YAAY,GAAG;MACtB1B,KAAK,CAACO,GAAG,CAAC,qBAAqB,CAAC,CAACC,IAAI,CAACC,GAAG,IAAE;QACzCC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;QACrBN,QAAQ,CAACO,KAAI,GAAIJ,GAAG,CAACG,IAAI;MAC3B,CAAC,CAAC,CAAEE,KAAK,CAACC,GAAG,IAAE;QACbL,OAAO,CAACC,GAAG,CA